{
  "name": "rtc-todo-example",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@jupyterlab/apputils": "2.1.1",
    "@lumino/datastore": "^0.9.1",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "fkill-cli": "6.0.1",
    "npm-run-all": "4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "rtc-node": "*",
    "rtc-relay": "*",
    "typescript": "~3.7.2",
    "wait-on": "5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:relay": "fkill -s :8888 && rtc-relay",
    "wait:relay": "wait-on tcp:8888",
    "wait-then-start": "npm-run-all wait:relay start",
    "start:all": "npm-run-all --parallel --race start:relay wait-then-start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
