  "homepage": "https://github.com/jupyterlab/rtc#readme",
  "dependencies": {
    "@lumino/coreutils": "^1.4.1",
    "@lumino/datastore": "^0.8.3",
    "@jupyterlab/nbformat": "^2.0.0-rc.0"
  },
  "devDependencies": {
    "rimraf": "^2.5.2",
    "typescript": "~3.7.5"
